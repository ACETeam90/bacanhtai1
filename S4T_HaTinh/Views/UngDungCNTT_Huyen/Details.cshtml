@model S4T_HaTinh.Models.UngDungCNTT_Huyen

@{
    ViewBag.Title = "Details";
    var listStatusNhapLieu = MvcApplication.ListTrangThaiNhapLieu();
}


<style type="text/css">
    .S4T_NumericTextBox {
        width: 90px;
        float: left;
    }

    .S4T_AutoComplete{
        width: 400px;
        float: left;
    }
</style>
<script src="~/Scripts/s4t-hatinh.js" type="text/javascript"></script>

<h3>ỨNG DỤNG CNTT HUYỆN</h3>
<h4>@ViewBag.TenDonVi</h4>
<h5>Trạng thái: @Html.DisplayTextFor(Model => listStatusNhapLieu.FirstOrDefault(o => o.DanhMuc_ID == Model.Success).TenDanhMuc)</h5>
<br />

@using (Html.BeginForm())
{
    <button type="button" class="ExportWord" onclick="location.href='@Url.Action("ExportToWord", new { id = Model.UngDungCNTTHuyen_ID })'" style="float:right;">Xuất báo cáo</button>
    <hr />
    
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.Partial("_CreateOrUpdate", Model, new ViewDataDictionary { { "IsDisabled", true } })       
    </div>
}

<script type="text/javascript">
    var listInputRadio = '@ViewBag.listInputRadio';

    $(document).ready(function () {
        CheckInputRadioByValue(); // script : s4t-hatinh.js
    });
</script>

